        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:id="@+id/iv_window_meeting"
            android:layout_marginRight="15px"
            >

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:src="@drawable/iv_sum_meeting"
                android:background="@drawable/ease_common_tab_bg"
                />

            <TextView
                android:id="@+id/more_window_online"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:clickable="true"
                android:text="@string/meeting"
                android:textColor="@color/main_kuang"
                android:layout_marginTop="300px"
                />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:id="@+id/iv_window_jiandao"
            >

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:src="@drawable/iv_sum_jiandao"
                android:background="@drawable/ease_common_tab_bg"
                />

            <TextView
                android:id="@+id/more_window_online2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="300px"
                android:clickable="true"
                android:text="@string/chenking"
                android:textColor="@color/main_kuang" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:id="@+id/iv_window_answer"
            android:layout_marginLeft="15px"
            >

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:src="@drawable/iv_sum_answer"
                android:background="@drawable/ease_common_tab_bg"
                />

            <TextView
                android:id="@+id/more_window_online3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="300px"
                android:clickable="true"
                android:text="@string/asking"
                android:textColor="@color/main_kuang" />
        </RelativeLayout>

    <ImageView
        android:id="@+id/iv_sum_file"
        android:layout_width="150px"
        android:layout_height="150px"
        app:layout_auto_baseheight="width"
        android:src="@drawable/iv_sum_file"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/iv_window_jiandao"
        android:layout_marginTop="700px"
        />
    <ImageView
        android:id="@+id/iv_sum_chiema"
        android:layout_width="150px"
        android:layout_height="150px"
        app:layout_auto_baseheight="width"
        android:src="@drawable/iv_sum_chiema"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/iv_window_jiandao"
        android:layout_marginTop="700px"
        />
    <ImageView
        android:id="@+id/iv_sum_do"
        android:layout_width="150px"
        android:layout_height="150px"
        app:layout_auto_baseheight="width"
        android:src="@drawable/iv_sum_do"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/iv_window_jiandao"
        android:layout_marginTop="700px"
        />


    <ImageView
        android:id="@+id/iv_sum_work"
        android:layout_width="150px"
        android:layout_height="150px"
        app:layout_auto_baseheight="width"
        android:src="@drawable/iv_sum_work"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/iv_window_jiandao"
        android:layout_marginTop="700px"
        />
    <ImageView
        android:id="@+id/iv_sum_faviate"
        android:layout_width="150px"
        android:layout_height="150px"
        app:layout_auto_baseheight="width"
        android:src="@drawable/iv_sum_faviate"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/iv_window_jiandao"
        android:layout_marginTop="700px"
        />
</com.zhy.autolayout.AutoRelativeLayout>
